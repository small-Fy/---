.goodsItem{
  margin-top: 16rpx;
  padding:10rpx 30rpx
}



.searchInner .searchList{
  background-color: white;
  margin-top: 40rpx
}

.searchInner .searchList .search_top{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx
}

.searchInner .searchList .search_top .text{
  font-size: 32rpx
}

.searchInner .searchList .search_top .delete{
  display: flex;
  align-items: center;
}

.searchInner .searchList .inner{
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
  padding: 0 20rpx
}

.searchInner .searchList .inner .hot_item{
  border: 2rpx solid #ccc;
  padding: 8rpx 12rpx;
  margin: 20rpx 20rpx 0 0
}

.searchInner .hotKeywordList{
  margin-top: 40rpx;
  padding: 0 20rpx
}

.searchInner .hotKeywordList .text{
  font-size: 32rpx
}

.searchInner .hotKeywordList .inner{
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx
}

.searchInner .hotKeywordList .inner .hot_item{
  border: 2rpx solid #ccc;
  padding: 8rpx 12rpx;
  margin: 20rpx 20rpx 0 0
}

.searchInner .hotKeywordList .inner .font{
  border: 2rpx solid #c20c0c
}